{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "analyses": {
          "composites": false
        },
        "subdirWatchPatterns": "auto"
      }
    },
    "sorbet": {
      "enable": true,
      "command": "~/.rbenv/shims/bundle",
      "args": [
        "exec",
        "srb",
        "tc",
        "--typed",
        "true",
        "--enable-all-experimental-lsp-features",
        "--lsp",
        "--disable-watchman"
      ],
      "filetypes": [
        "ruby",
        "rbi"
      ],
      "additionalSchemes": [
        "sorbet:build",
        "sorbet:https"
      ],
      "initializationOptions": {
        "supportsSorbetURIs": true
      },
      "rootPatterns": [
        "sorbet/config"
      ],
      "settings": {}
    }
  },
  "clangd.disableFileStatus": true,
  "clangd.disableProgressNotifications": true,
  "coc.preferences.messageLevel": "error",
  "coc.preferences.willSaveHandlerTimeout": 2000,
  "coc.source.tag.priority": 20,
  "codeLens.enable": true,
  "diagnostic.autoRefresh": true,
  "diagnostic.displayByAle": true,
  "hover.floatConfig": {
    "border": true
  },
  "list.previewSplitRight": true,
  "list.previewToplineStyle": "middle",
  "list.normalMappings": {
    "p": "do:previewtoggle",
    "v": "action:vsplit",
    "<C-v>": "action:vsplit",
    "<C-d>": "action:split",
    "<C-t>": "action:tabe",
    "<C-j>": "normal:j",
    "<C-n>": "normal:j",
    "<C-k>": "normal:k",
    "<C-p>": "normal:k"
  },
  "notification.disabledProgressSources": [
    "*"
  ],
  "[ruby]": {
    "coc.preferences.formatOnSave": true
  },
  "rust-analyzer.lens.debug.enable": false,
  "rust-analyzer.lens.run.enable": false,
  "suggest.enableFloat": true,
  "suggest.enablePreselect": false,
  "suggest.maxCompleteItemCount": 20,
  "suggest.minTriggerInputLength": 0,
  "suggest.noselect": true,
  "suggest.snippetIndicator": ""
}
