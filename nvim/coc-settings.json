{
  "languageserver": {
    "flow": {
      "command": "${workspaceFolder}/node_modules/.bin/flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact", "javascript.jsx"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "coc": {
    "preferences": {
      "messageLevel": "error"
    },
    "source": {
      "tag": {
        "priority": 20
      }
    }
  },
  "codeLens.enable": true,
  "diagnostic.displayByAle": true,
  "hover": {
    "floatConfig": {
      "border": true
    }
  },
  "suggest": {
    "enablePreview": true,
    "floatEnable": true,
    "maxCompleteItemCount": 20,
    "snippetIndicator": ""
  }
}

